___SANDBOXED_JS_FOR_SERVER___

const getAllEventData = require('getAllEventData');
const sendHttpRequest = require('sendHttpRequest');
const encodeUriComponent = require('encodeUriComponent');
const generateRandom = require('generateRandom');
const getType = require('getType');
const JSON = require('JSON');
const log = require('logToConsole');
const setCookie = require('setCookie');
const getCookieValues = require('getCookieValues');
const sha256Sync = require('sha256Sync');
const getTimestamp = require('getTimestamp');


const serviceUrl = data.endpointUrl;
const siteId = data.siteId || '1';

const event = getAllEventData();
const pkId = getCookieValues('_pk_id');
let clientId = '';
if (!pkId || !pkId[0]) {
  clientId = sha256Sync(generateRandom(0, 10000000) + '|' + getTimestamp(), {outputEncoding: 'hex'}).slice(0, 16);
  const options = {
    domain: 'auto',
    path: '/',
    secure: true,
    httpOnly: false,
    'max-age': 86400 * 400
  };
  setCookie('_pk_id', clientId, options, false);
} else {
  clientId = pkId[0];
}

const parameters = {
  _id: clientId,
  apiv: 1,
  idsite: siteId,
  rec: 1,
  res: event.screen_resolution || "",
  ua: event.user_agent || "",
  lang: event.language || "",
  url: event.page_location || "",
  uid: event.user_id || undefined,
  rand: generateRandom(100000, 999999),
  urlref: event.page_referrer || "",
  send_image: 0 // no need for gif, 204 is enough
};

if (data.authToken) {
  parameters.token_auth = data.authToken;
  parameters.cip = event.ip_override;
}

let shouldTrigger = false;

switch (event.event_name) {
  case data.pageViewEvent:
    shouldTrigger = true;
    parameters.action_name = event.page_title || "";
    break;
  case data.purchaseEvent:
    shouldTrigger = true;

    parameters.idgoal = 0;

    parameters.ec_id = event.transaction_id;
    parameters.revenue = event.value || event['x-ga-mp1-tr'];
    // parameters.ec_st = SUBTOTAL
    parameters.ec_tx = event.tax || undefined;
    parameters.ec_sh = event.shipping || undefined;
    parameters.ec_dt = event.discount || undefined;

    if (event.items && getType(event.items) === 'array') {
      parameters.ec_items = [];
      event.items.forEach((item) => {
        const categories = [];
        ['', '2', '3', '4', '5'].forEach(cId => {
          if (item['item_category' + cId]) {
            categories.push(item['item_category' + cId]);
          }
        });

        if (categories.length === 0 && item.category) {
          categories.push(item.category);
        }

        parameters.ec_items.push([
          item.item_id || item.id || item.item_name, // item sku (required)
          item.item_name || item.name || '', // item name (or if not applicable, set it to an empty string)
          categories.join(' > '), // item category (or if not applicable, set it to an empty string)
          item.price || 0, // item price (or if not applicable, set it to 0)
          item.quantity || 1 // item quantity (or if not applicable, set it to 1)
        ]);
      });
    }

    break;

  case data.custom_event:
    shouldTrigger = true;

    parameters.e_c = data.event_category || "";
    parameters.e_a = data.event_action || "";
    parameters.e_n = data.event_label || "";
    parameters.e_v = data.event_value || "";

    break;
}

if (shouldTrigger) {
  if (data.optionalParametersTable) {
    for (let index in data.optionalParametersTable) {
      const parameter = data.optionalParametersTable[index];
      if (parameter.value && parameter.attribute) {
        parameters[parameter.attribute] = parameter.value;
      }
    }
  }

  log('Sending to ' + serviceUrl + '?' + toQueryString(parameters));

  sendHttpRequest(
    serviceUrl + '?' + toQueryString(parameters),
    (statusCode, headers, body) => {
      log({ statusCode: statusCode, body: body });

      if (statusCode >= 200 && statusCode < 300) {
        data.gtmOnSuccess();
      } else {
        data.gtmOnFailure();
      }
    },
    {
      headers: {'user-agent': event.user_agent || ""},
      timeout: 500,
      method: 'POST'
    }
  );
} else {
  log('Event (' + event.event_name +') shouldn\'t trigger request');
  data.gtmOnSuccess();
}


function toQueryString(params) {
  let string = '';

  for (const key in params) {
    const value = params[key];
    const type = getType(value);

    if (type === 'undefined') {
      continue;
    }

    if (string !== '') {
      string += '&';
    }

    string += key + '=';

    if (['array', 'object', 'function'].indexOf(type) !== -1) {
      string += encodeUriComponent(JSON.stringify(value));
    } else {
      string += encodeUriComponent(value);
    }
  }

  return string;
}


